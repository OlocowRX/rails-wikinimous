<div class="navbar">
  <h1>Articles</h1>
  <%= link_to 'Add a new article', new_article_path %>
</div>

<div class="container">
  <% @articles.each do |article| %>
    <div class="card">
      <h2><%= link_to article.title, article_path(article)%></h2>
      <p><%= article.content %></p>
    </div>
  <% end %>
</div>
